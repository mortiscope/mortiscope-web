{
  "name": "mortiscope-web",
  "version": "0.1.0",
  "description": "A web application for a deep learning system that analyzes Chrysomya megacephala images to assist in forensic Post-Mortem Interval (PMI) estimation.",
  "keywords": [
    "typescript",
    "computer vision",
    "deep learning",
    "nextjs",
    "tailwindcss",
    "authjs",
    "drizzle-orm",
    "typescript",
    "forensic science",
    "forensic entomology"
  ],
  "contributors": [
    {
      "name": "Precious Rowelyn Andal",
      "url": "https://github.com/cinnamongirl29"
    },
    {
      "name": "Neil Ivan Orencia",
      "url": "https://github.com/neilivanorencia"
    }
  ],
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/mortiscope/mortiscope-web.git"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "dev:web": "concurrently \"pnpm:dev\" \"pnpm:inngest:dev\"",
    "dev:webpack": "next dev",
    "inngest:dev": "npx inngest-cli@latest dev -u http://localhost:3000/api/inngest",
    "email": "dotenv -- email dev --dir src/emails",
    "build": "next build",
    "build:turbo": "next build --turbo",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:validate": "pnpm run quality && pnpm run build",
    "build:production": "cross-env NODE_ENV=production pnpm run build:validate",
    "analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:strict": "tsx scripts/lint.ts --max-warnings 0",
    "typecheck": "tsx scripts/typecheck.ts",
    "quality": "pnpm run lint:strict && pnpm run typecheck",
    "quality:fix": "pnpm run lint:fix && pnpm run typecheck",
    "pre-commit": "lint-staged",
    "prepare": "husky",
    "db:push": "tsx node_modules/drizzle-kit/bin.cjs push",
    "db:generate": "tsx node_modules/drizzle-kit/bin.cjs generate",
    "db:studio": "tsx node_modules/drizzle-kit/bin.cjs studio",
    "audit": "pnpm audit --audit-level moderate",
    "audit:fix": "pnpm audit fix",
    "deps:check": "pnpm outdated",
    "deps:update": "npx npm-check-updates -i",
    "bundle:analyze": "cross-env ANALYZE=true next build",
    "bundle:size": "npx size-limit",
    "lighthouse": "lighthouse http://localhost:3000 --view --chrome-flags=\"--headless\"",
    "geoip:update": "dotenv -- cross-env pnpm --dir node_modules/geoip-lite run updatedb license_key=%MAXMIND_LICENSE_KEY%",
    "clean": "tsx scripts/clean.ts",
    "clean:build": "tsx scripts/clean.ts build",
    "clean:deps": "tsx scripts/clean.ts deps",
    "clean:all": "tsx scripts/clean.ts all",
    "clean:force": "tsx scripts/clean.ts all --force"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "size-limit": [
    {
      "name": "Core App Logic (Shared Baseline)",
      "path": [
        ".next/static/chunks/webpack-*.js",
        ".next/static/chunks/framework-*.js",
        ".next/static/chunks/main-app-*.js"
      ],
      "limit": "110 kB"
    },
    {
      "name": "Analyze (/analyze)",
      "path": [
        ".next/static/chunks/webpack-*.js",
        ".next/static/chunks/framework-*.js",
        ".next/static/chunks/main-app-*.js",
        ".next/static/chunks/app/\\(private\\)/analyze/page-*.js"
      ],
      "limit": "370 kB"
    },
    {
      "name": "Results (/results)",
      "path": [
        ".next/static/chunks/webpack-*.js",
        ".next/static/chunks/framework-*.js",
        ".next/static/chunks/main-app-*.js",
        ".next/static/chunks/app/\\(private\\)/results/**/page-*.js"
      ],
      "limit": "240 kB"
    },
    {
      "name": "Account Page (/account)",
      "path": [
        ".next/static/chunks/webpack-*.js",
        ".next/static/chunks/framework-*.js",
        ".next/static/chunks/main-app-*.js",
        ".next/static/chunks/app/account/page-*.js"
      ],
      "limit": "215 kB"
    },
    {
      "name": "Authentication Pages",
      "path": [
        ".next/static/chunks/webpack-*.js",
        ".next/static/chunks/framework-*.js",
        ".next/static/chunks/main-app-*.js",
        ".next/static/chunks/app/\\(auth\\)/**/page-*.js"
      ],
      "limit": "170 kB"
    },
    {
      "name": "Middleware",
      "path": ".next/server/src/middleware.js",
      "limit": "50 kB",
      "webpack": false
    },
    {
      "name": "Global CSS",
      "path": ".next/static/css/**/*.css",
      "limit": "25 kB",
      "webpack": false
    }
  ],
  "dependencies": {
    "@auth/core": "^0.40.0",
    "@auth/drizzle-adapter": "^1.10.0",
    "@aws-sdk/client-s3": "^3.884.0",
    "@aws-sdk/s3-request-presigner": "^3.884.0",
    "@hookform/resolvers": "^5.2.1",
    "@neondatabase/serverless": "^1.0.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "@react-email/components": "^0.5.2",
    "@tanstack/react-query": "^5.87.1",
    "@tiptap/extension-blockquote": "^3.6.1",
    "@tiptap/extension-bullet-list": "^3.6.1",
    "@tiptap/extension-heading": "^3.6.1",
    "@tiptap/extension-list-item": "^3.6.1",
    "@tiptap/extension-ordered-list": "^3.6.1",
    "@tiptap/extension-task-item": "^3.6.1",
    "@tiptap/extension-task-list": "^3.6.1",
    "@tiptap/extension-typography": "^3.6.1",
    "@tiptap/extension-underline": "^3.6.1",
    "@tiptap/react": "^3.6.1",
    "@tiptap/starter-kit": "^3.6.1",
    "@trpc/client": "^11.5.1",
    "@trpc/server": "^11.5.1",
    "@uidotdev/usehooks": "^2.4.1",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
