{
  "name": "mortiscope-web",
  "version": "0.1.0",
  "description": "A web application for a deep learning system that analyzes Chrysomya megacephala images to assist in forensic Post-Mortem Interval (PMI) estimation.",
  "keywords": [
    "typescript",
    "computer vision",
    "deep learning",
    "nextjs",
    "tailwindcss",
    "authjs",
    "drizzle-orm",
    "typescript",
    "forensic science",
    "forensic entomology"
  ],
  "contributors": [
    {
      "name": "Precious Rowelyn Andal",
      "url": "https://github.com/cinnamongirl29"
    },
    {
      "name": "Neil Ivan Orencia",
      "url": "https://github.com/neilivanorencia"
    }
  ],
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/mortiscope/mortiscope-web.git"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:web": "concurrently \"pnpm:dev\" \"pnpm:inngest:dev\"",
    "inngest:dev": "npx inngest-cli@latest dev -u http://localhost:3000/api/inngest",
    "email": "dotenv -- email dev --dir src/emails",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:validate": "pnpm run quality && pnpm run build",
    "build:production": "cross-env NODE_ENV=production pnpm run build:validate",
    "analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "quality": "pnpm run lint:strict && pnpm run typecheck",
    "quality:fix": "pnpm run lint:fix && pnpm run typecheck",
    "pre-commit": "lint-staged",
    "prepare": "husky",
    "db:push": "tsx node_modules/drizzle-kit/bin.cjs push",
    "db:generate": "tsx node_modules/drizzle-kit/bin.cjs generate",
    "db:studio": "tsx node_modules/drizzle-kit/bin.cjs studio",
    "audit": "pnpm audit --audit-level moderate",
    "audit:fix": "pnpm audit fix",
    "deps:check": "pnpm outdated",
    "deps:update": "npx npm-check-updates -i",
    "bundle:analyze": "cross-env ANALYZE=true next build",
    "bundle:size": "npx size-limit",
    "lighthouse": "lighthouse http://localhost:3000 --view --chrome-flags=\"--headless\"",
    "clean": "rimraf .next out dist",
    "clean:deps": "rimraf node_modules && pnpm install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "next lint --fix --file"
    ]
  },
  "size-limit": [
    {
      "name": "Core App Logic (Shared Baseline)",
      "path": [
        ".next/static/chunks/webpack-*.js",
        ".next/static/chunks/framework-*.js",
        ".next/static/chunks/main-app-*.js"
      ],
      "limit": "110 kB"
    },
    {
      "name": "Analyze (/analyze)",
      "path": [
        ".next/static/chunks/webpack-*.js",
        ".next/static/chunks/framework-*.js",
        ".next/static/chunks/main-app-*.js",
        ".next/static/chunks/app/\\(private\\)/analyze/page-*.js"
      ],
      "limit": "370 kB"
    },
    {
      "name": "Results (/results)",
      "path": [
        ".next/static/chunks/webpack-*.js",
        ".next/static/chunks/framework-*.js",
        ".next/static/chunks/main-app-*.js",
        ".next/static/chunks/app/\\(private\\)/results/**/page-*.js"
      ],
      "limit": "240 kB"
    },
    {
      "name": "Authentication Pages",
      "path": [
        ".next/static/chunks/webpack-*.js",
        ".next/static/chunks/framework-*.js",
        ".next/static/chunks/main-app-*.js",
        ".next/static/chunks/app/\\(auth\\)/**/page-*.js"
      ],
      "limit": "170 kB"
    },
    {
      "name": "Middleware",
      "path": ".next/server/src/middleware.js",
      "limit": "50 kB",
      "webpack": false
    },
    {
      "name": "Global CSS",
      "path": ".next/static/css/**/*.css",
      "limit": "25 kB",
      "webpack": false
    }
  ],
  "dependencies": {
    "@auth/core": "^0.39.1",
    "@auth/drizzle-adapter": "^1.9.1",
    "@aws-sdk/client-s3": "^3.842.0",
    "@aws-sdk/s3-request-presigner": "^3.842.0",
    "@hookform/resolvers": "^5.0.1",
    "@neondatabase/serverless": "^1.0.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "@react-email/components": "^0.0.41",
    "@tanstack/react-query": "^5.80.10",
    "@trpc/client": "^11.3.1",
    "@trpc/server": "^11.3.1",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.35.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.4",
    "file-type": "^21.0.0",
    "framer-motion": "^12.14.0",
    "immer": "^10.1.1",
    "inngest": "^3.40.1",
    "lucide-react": "^0.511.0",
    "next": "15.3.2",
    "next-auth": "5.0.0-beta.28",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-day-picker": "^9.8.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.56.4",
    "react-icons": "^5.5.0",
    "react-spinners": "^0.17.0",
    "react-webcam": "^7.2.0",
    "react-zoom-pan-pinch": "^3.7.0",
    "recharts": "^2.15.3",
    "resend": "^4.5.2",
    "select-philippines-address": "^1.0.6",
    "sonner": "^2.0.5",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.0",
    "tw-animate-css": "^1.3.0",
    "uuid": "^11.1.0",
    "vaul": "^1.1.2",
    "zod": "^3.25.30",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@next/bundle-analyzer": "^15.5.2",
    "@size-limit/file": "^11.2.0",
    "@size-limit/preset-big-lib": "^11.2.0",
    "@tailwindcss/postcss": "^4.1.7",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.27.0",
    "eslint-config-next": "15.4.0-canary.51",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "inngest-cli": "^1.11.10",
    "lighthouse": "^12.8.2",
    "lint-staged": "^16.1.6",
    "npm-check-updates": "^18.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-email": "^4.0.16",
    "rimraf": "^6.0.1",
    "size-limit": "^11.2.0",
    "tailwindcss": "^4.1.7",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
